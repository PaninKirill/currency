# .coveragerc to control coverage.py[run]branch = Trueomit = src/manage.py,       */migrations/*,       */tests/*,       src/settings/settings*.py,       src/settings/wsgi*.py,[report]precision = 4exclude_lines =    pragma: no cover    def __repr__    def __str__    def parse_tasks    continue    settings.DEBUG    ProgrammingError    if latest is not None[html]directory = static_content/media/coverage